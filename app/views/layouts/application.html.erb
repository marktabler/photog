  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Sarina|Merriweather:700,300|Merriweather+Sans:300,700' rel='stylesheet' type='text/css'>
  
</head>
<body>
    <div id="header_background">   
      <div id="header_container">
        <h1>Beth Tabler Photography</h1>
        <nav>
          <li><a href="/html/">HOME</a></li>
          <li id="break"> / </li>
          <li><a href="/html/">ABOUT</a></li>
          <li id="break"> / </li>
          <li><a href="/html/">CONTACT</a></li>
          <li id="break"> / </li>
